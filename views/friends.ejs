<%# Header %>

<%- include("./partials/banner"); -%>
<%- include("./partials/nav"); -%>

<%# content %>

<%# Search Bar %>
<div class = 'row'>
  <div class="col-12">
  <%- include ("./partials/messages"); -%>
  <form class="form-inline mr-auto mb-4" action="/users/friends/results" method="POST">
    <div class="input-group col-md-11">
            <input class="form-control py-2 border-right-0 border" type="search" placeholder="search" id="search" name="search">
            <span class="input-group-append">
              <button class="btn btn-outline-secondary border-left-0 border" type="submit">
                    <i class="fa fa-search"></i>
              </button>
            </span>
        </div>
  </form>
</div>
</div>




<div class="form-group" style="display:none">
  <label class="control-label col-sm-2" for="_id">ID</label>
  <div class="col-sm-10">
  <input
    type="_id"
    id="_id"
    name="_id"
    class="form-control"
    value="<%= _id %>"
  />
  </div>
</div>

<%# List of Friends %>
<div class = 'row'>
  <div class="col-12">
  <div class="form-group">
    <div class="row">
      <%- include("./partials/friends"); -%>
    </div>
  </div>
</div>
</div>
